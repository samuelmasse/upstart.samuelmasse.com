{
  "name": "@upstart.samuelmasse.com/infra",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dep": "pnpm build && pnpm eslint --fix && pnpm exec cdk deploy",
    "co": "pnpm --filter @upstart.samuelmasse.com/common dev",
    "fe": "pnpm --filter @upstart.samuelmasse.com/frontend dev",
    "be": "pnpm --filter @upstart.samuelmasse.com/backend dev",
    "build": "pnpm install && pnpm --recursive build && tsc",
    "clean": "pnpm --recursive clean && rimraf cdk.out dist"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "aws-cdk": "^2.1104.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.2.0",
    "jiti": "^2.6.1",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@upstart.samuelmasse.com/common": "workspace:*",
    "aws-cdk-lib": "^2.236.0",
    "constructs": "^10.4.5"
  }
}
